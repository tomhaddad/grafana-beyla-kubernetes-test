{
    "root": true,
    "parser": "@typescript-eslint/parser",
    "extends": ["prettier", "plugin:@typescript-eslint/recommended", "plugin:prettier/recommended"],
    "parserOptions": {
      "ecmaVersion": 2022,
      "sourceType": "module"
    },
    "rules": {
      "no-unused-vars": "off",
      "@typescript-eslint/no-unused-vars": [
        "error",
        {
          "argsIgnorePattern": "^_",
          "varsIgnorePattern": "^_",
          "caughtErrorsIgnorePattern": "^_"
        }
      ]
    }
  }
  